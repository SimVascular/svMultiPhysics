	</Add_face>
</Add_mesh>

<Add_equation type="heatS" >
	<Coupled> 0 </Coupled>
	<Min_iterations> 1 </Min_iterations>
	<Max_iterations> 5 </Max_iterations>
	<Tolerance> 1e-6 </Tolerance>

	<Conductivity> 1.0 </Conductivity>
	<Source_term> 0.0 </Source_term>
	<Density> 0.0 </Density>

	<Output type="Spatial" >
		<Temperature> 1 </Temperature>
	</Output>

	<Output type="Alias" >
		<Temperature> Phi_LV_EPI </Temperature>
	</Output>

	<LS type="CG" >
		<Linear_algebra type="fsils" >
         <Preconditioner> fsils </Preconditioner> 
    	</Linear_algebra>
		<Max_iterations> 500 </Max_iterations>
		<Tolerance>      1e-6 </Tolerance>
	</LS>

	<Add_BC name="epi" >
		<Type> Dir </Type>
		<Value> 1.0 </Value>
		<Zero_out_perimeter> 0 </Zero_out_perimeter>
	</Add_BC>

	<Add_BC name="endo" >
